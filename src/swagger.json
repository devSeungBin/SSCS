{
  "swagger": "2.0",
  "info": {
    "titile": "API 문서",
    "description": "API 문서 자동화 테스트",
    "version": "1.0.0",
    "title": "REST API"
  },
  "host": "localhost:3000",
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "basePath": "/",
  "schemes": [
    "http"
  ],
  "securityDefinitions": {
    "bearerAuth": {
      "type": "http",
      "scheme": "bearer",
      "in": "header",
      "bearerFormat": "JWT"
    }
  },
  "paths": {
    "/test": {
      "get": {
        "tags": [
          "testRouter"
        ],
        "summary": "실험용 라우터",
        "description": "여러 기능을 실험하는 테스트 라우터. 아무것도 없다.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "User ID.",
            "required": true,
            "type": "integer"
          },
          {
            "name": "body",
            "in": "body",
            "description": "User data.",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "type": "string",
                  "example": "user"
                },
                "password": {
                  "type": "string",
                  "example": "1234"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "요청 성공 응답"
          },
          "201": {
            "description": "요청 성공 응답, 자원 생성 됨"
          },
          "500": {
            "description": "서버 내부 오류"
          }
        }
      }
    }
  }
}